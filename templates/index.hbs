<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>PDF</title>
    <style>
      .general-info{ border: 2px black solid; border-radius: 5px; padding: 20px 10px 10px 5px; display: flex;
      flex-direction: column; gap: 12px; } .row{ display: flex; width: 100%; }.row--part{ display: flex; gap: 5px;
      align-items: flex-end; white-space: nowrap; } .row .left-part{ width: 60%; } .row .right-part{ width: 40%;
      }.row--part span{white-space: pre-wrap;width: 100%; text-align: center; border-bottom: 2px dotted rgba(0, 0, 0,
      0.5); }.table { width: 100%; border: 2px black solid; border-radius: 5px 5px 0 5px; margin: 30px 0 15px;
      border-collapse: collapse; border-style: hidden; box-shadow: 0 0 0 2px #000; } .thead { letter-spacing: 0.5em;
      text-transform: uppercase; border: 1px solid black; } .thead th { padding: 4px; border: 2px black solid; }
      .concepto { width:65%; } .precio-total { width: 20%; } .tbody td{ border: 1px black solid; border-right-width:
      2px; padding: 7px 15px 5px 15px; font-size: 1.1em; text-align: center;}.concepto--td{ font-weight: 600;
      text-align: left !important;} .table-container{ position: relative; } .table-total{ position: absolute; right:
      -2px; bottom: 0; transform: translate(0,100%); display: flex; font-size: 1.1em; font-weight: 500; } .table-total
      .names>div, .table-total .values>div{ padding: 19px 0px 13px 0px; } .table-total .values{ width: 150px; border:
      2px solid black; border-top:none; border-radius: 0 0 5px 5px; text-align: center; } .table-total .names>div{
      border-bottom: 1px solid black; padding-right: 12px; } .table-total .names>div:nth-last-child(1){ border-bottom:
      none; font-size: 1.3em; } .table-total .values>div{ border-bottom: 1px solid black; } .table-total
      .values>div:nth-last-child(1){ border-bottom: none; font-size: 1.3em; text-transform: uppercase; } .aparat-info{
      border: 2px solid black; border-radius: 5px; width: 55%; padding: 15px 10px 10px 5px; display: flex;
      flex-direction: column; gap: 13px; width: 450px; font-weight: 600; } .aparat-info .row--part{ width: 100%; }
      .aparat-info span{ font-weight: 500; } .signatures{ margin-top: 30px; border: 2px solid black; border-radius: 5px;
      width: 100%; height: 180px; display: flex; } .signature-container{ box-sizing: border-box; height: 100%; width:
      33.3%; border-right: 2px solid black; display: flex; flex-direction: column; align-items: center; padding: 5px; }
      .signature-container:nth-last-of-type(1){ border-right: none; } .signature-container span{ display: block; }
      .signature{ height: 100%; max-width: 100%; max-height: 100%; object-fit: scale-down; } .signature-container
      .row--part{ margin-top: 5px; width: 100%; font-size: 0.8em; } .image-container{ width: 100%; height: 100%;
      position: relative; } .image-inner-container{ position: absolute; top: 0px; height: 0px; width: 100%; height:
      100%; display: flex; align-items: center; justify-content: center; } .header{ display: flex; align-items: center;
      justify-content: space-between; margin-top: 10px; margin-bottom: 30px; padding: 0 5px; } .logo{ width:
      390px;height: 88px; display: flex; align-items: center;} .logo__image svg{height: 88px; width: 88px} .logo__text
      svg{height: 65px; width: 230px} .logo__text div{font-size: 14px; padding-left: 5px} .tel{ color: black; font-size:
      55px; line-height: 60px;font-weight: 700; }.address{ margin: 0px 0 5px 0; font-size: 16px; }
    </style>
  </head>
